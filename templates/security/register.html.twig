{% extends 'base.html.twig' %}

{% block body %}
    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.lastname) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    {{ form_row(form.card_number) }}
    {{ form_row(form.blocked) }}

    <button type="submit">Register!</button>
{{ form_end(form) }}
{% endblock %}